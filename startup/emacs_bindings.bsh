/**
 * Load Emacs bindings. You must copy this file to your
 * ${user.home}/.jedit/startup directory
 */

settings = jEdit.getSettingsDirectory();
fileSep = System.getProperty ("file.separator");
source (settings +
        fileSep +
        "macros" +
        fileSep +
        "Emacs" +
        fileSep +
        "Emacs_Set_Keybindings.bsh");
emacsSetKeybindings();
